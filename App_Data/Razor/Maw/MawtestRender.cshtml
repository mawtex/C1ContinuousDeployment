@inherits RazorFunction
@functions {
    public RoutedData<Maw.Mawtest> ListData { get; set; }
}
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:f="http://www.composite.net/ns/function/1.0">
	<head />
    <body>
		<ul>
			@foreach( var item in ListData.List)
			{
				<li>
					<a href="@ListData.ItemUrl(item)">@ListData.ItemUrl(item)</a> <br/>
					@item.Title
				</li>
			}
		</ul>
    </body>
</html>
